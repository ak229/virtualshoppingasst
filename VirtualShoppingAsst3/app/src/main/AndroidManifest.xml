<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ub.tuere.virtualshoppingasst">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BarcodeScanner">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".Barcode_shower" />
        <activity android:name=".qrcodeshower" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegistrationForm" />
        <activity android:name=".ScannerResult" />
        <activity android:name=".HomeProfile" />
        <activity android:name=".WorkProfile" />
        <activity android:name=".BasicProfile" />
        <activity android:name=".CompleteProfile" />
    </application>

</manifest>